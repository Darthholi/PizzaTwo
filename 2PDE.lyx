\begin_inset Formula $\forall(t,x)\in(0,T)\times\Omega:\, w\leq\max(\frac{f_{\lambda}^{max}}{c_{min}=1},\max\, w_{D}|_{\delta\Omega},\max w(0))$
\end_inset


\end_layout

\begin_layout Standard
